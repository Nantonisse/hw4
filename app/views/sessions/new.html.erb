<h1>Login</h1>

<%= form_with :url => "/sessions" do |form| %>
  <p>
    <%= form.label "email" %>
    <%= form.text_field "email" %>
    <%= form.label "password" %>
    <%= form.password_field "password" %>
  </p>

    <button type="button" class="btn btn-outline-primary"><%= form.submit "Login" %></button>

<% end %>